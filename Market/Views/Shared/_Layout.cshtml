<!DOCTYPE html>
<html>

<head>

    <meta charset="utf-8">
    <title>فروشگاه اینترنتی</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="ProteusThemes">
    <!--  Google Fonts  -->
    <link href='http://fonts.googleapis.com/css?family=Pacifico|Open+Sans:400,700,400italic,700italic&amp;subset=latin,latin-ext,greek' rel='stylesheet' type='text/css'>

    <!-- Twitter Bootstrap -->
    <link href="@Url.Content("~/Content/WebMarket/webmarket/stylesheets/bootstrap.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/WebMarket/webmarket/stylesheets/responsive.css")" rel="stylesheet">
    <!-- Slider Revolution -->
    <link rel="stylesheet" href="@Url.Content("~/Content/WebMarket/webmarket/js/rs-plugin/css/settings.css")" type="text/css" />
    <!-- jQuery UI -->
    <link rel="stylesheet" href="@Url.Content("~/Content/WebMarket/webmarket/js/jquery-ui-1.10.3/css/smoothness/jquery-ui-1.10.3.custom.min.css")" type="text/css" />
    <!-- PrettyPhoto -->
    <link rel="stylesheet" href="@Url.Content("~/Content/WebMarket/webmarket/js/prettyphoto/css/prettyPhoto.css")" type="text/css" />
    <!-- main styles -->
    <link href="@Url.Content("~/Content/WebMarket/webmarket/stylesheets/main.css")" rel="stylesheet">


    <link rel="stylesheet" href=@Url.Content("~/Scripts/PersianDateTimePicker/PersianDatePicker.min.css")>


    @RenderSection("styles", false)





    <!-- Modernizr -->
    <script src="@Url.Content("~/Content/WebMarket/webmarket/js/modernizr.custom.56918.js")"></script>
    <!-- Fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="@Url.Content("~/Content/WebMarket/webmarket/images/apple-touch/144.png")">

    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="@Url.Content("~/Content/WebMarket/webmarket/images/apple-touch/114.png")">

    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="@Url.Content("~/Content/WebMarket/webmarket/images/apple-touch/72.png")">

    <link rel="apple-touch-icon-precomposed" href="@Url.Content("~/Content/WebMarket/webmarket/images/apple-touch/57.png")">

    <link rel="shortcut icon" href="@Url.Content("~/Content/WebMarket/webmarket/images/apple-touch/57.png")">

</head>

<body>

    <div class="master-wrapper" id="warpper">


        <header id="header">
            <div class="container">
                <div class="row">

                    <!--  ==========  -->
                    <!--  = Logo =  -->
                    <!--  ==========  -->
                    <div class="span7">
                        <a class="brand" href="@Url.Action(MVC.Home.Index())">
                            <img src="@Url.Content( "~/Content/WebMarket/webmarket/images/logo.png")" alt="Webmarket Logo" width="48" height="48" />
                            ><span class="pacifico">Webmarket</span>
                            <span class="tagline">فروشگاه هموراه تخفیف</span>
                        </a>
                    </div>

                    <!--  ==========  -->
                    <!--  = Social Icons =  -->
                    <!--  ==========  -->
                    <div class="span5">
                        <div class="topmost-line">
                        </div>
                    </div>

                    <div class="top-right">
                        <div class="icons">
                            <a href="#"><span class="zocial-facebook"></span></a>
                            <a href="#"><span class="zocial-skype"></span></a>
                            <a href="#"><span class="zocial-twitter"></span></a>
                            <a href="#"><span class="zocial-rss"></span></a>
                            <a href="#"><span class="zocial-wordpress"></span></a>
                            <a href="#"><span class="zocial-android"></span></a>
                            <a href="#"><span class="zocial-html5"></span></a>
                            <a href="#"><span class="zocial-windows"></span></a>
                            <a href="#"><span class="zocial-apple"></span></a>
                        </div>

                        @if (User.Identity.IsAuthenticated)
                        {
                            if (Request.IsAuthenticated && User.IsInRole("Admin"))
                            {
                                <a href=@Url.Action(MVC.Admin.User.Index())>پنل مدیریت</a>

                            }
                            <div class="register">
                                <a href="#">
                                    @* @{Html.RenderAction("FullName", "Account");}*@
                                </a>
                                <div class="register">
                                    <a href=@Url.Action(MVC.Account.LogOff())>خروج</a>
                                </div>
                            </div>
                        }
                        else
                        {
                            <div class="register">
                                <a href="#loginModal" role="button" data-toggle="modal">ورود</a> یا
                                <a href="..@Url.Action(MVC.User.Add())" role="button" data-toggle="modal">ثبت نام</a>
                            </div>
                        }


                    </div>
                </div> <!-- /social icons -->
            </div>
        </header>

        <!-- ========== -->
        <!-- = Main Menu / navbar = -->
        <!-- ========== -->
        <div class="navbar navbar-static-top" id="stickyNavbar">
            <div class="navbar-inner">
                <div class="container">
                    <div class="row">
                        <div class="span9">
                            <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>

                            <!-- ========== -->
                            <!-- = Menu = -->
                            <!-- ========== -->
                            <div class="nav-collapse collapse">
                                <ul class="nav" id="mainNavigation">
                                    @*<li class="dropdown">*@


                                    @*     @Html.Action("Menu", "Home")*@



                                    <li class="dropdown">
                                        <a href="shop.html" class="dropdown-toggle"> فروشگاه <b class="caret"></b> </a>
                                        <ul class="dropdown-menu">
                                            <li><a href="shop.html">قالب بندی پیش فرض</a></li>
                                            <li><a href="shop-no-sidebar.html">تمام صفحه</a></li>
                                            <li><a href="product.html">محصول تکی</a></li>
                                            <li><a href="shop-search.html">نتایج جستجو</a></li>
                                        </ul>
                                    </li>
                                    <li class="dropdown">
                                        <a href="blog.html" class="dropdown-toggle">بلاگ <b class="caret"></b> </a>
                                        <ul class="dropdown-menu">
                                            <li><a href="blog.html">قالب بندی پیش فرض</a></li>
                                            <li><a href="blog-single.html">تک نوشته</a></li>
                                            <li><a href="blog-search.html">نتایج جستجو</a></li>
                                            <li><a href="404.html">صفحه 404</a></li>
                                        </ul>
                                    </li>
                                    <li class="dropdown">
                                        <a href="features.html" class="dropdown-toggle">امکانات <b class="caret"></b> </a>
                                        <ul class="dropdown-menu">
                                            <li><a href="icons.html">آیکن ها</a></li>
                                            <li class="dropdown">
                                                <a href="features.html" class="dropdown-toggle"><i class="icon-caret-left pull-right visible-desktop"></i> همه امکانات</a>
                                                <ul class="dropdown-menu">
                                                    <li><a href="features.html#headings">سرخط ها</a></li>
                                                    <li><a href="features.html#alertBoxes">جعبه های هشدار</a></li>
                                                    <li><a href="features.html#tabs">تب ها</a></li>
                                                    <li><a href="features.html#buttons">دکمه ها</a></li>
                                                    <li><a href="features.html#toggles">تاگل ها</a></li>
                                                    <li><a href="features.html#quotes">نقل قول ها</a></li>
                                                    <li><a href="features.html#gallery">گرید های گالری</a></li>
                                                    <li><a href="features.html#code">کد</a></li>
                                                    <li><a href="features.html#columns">ستون ها</a></li>
                                                    <li><a href="features.html#maps">نقشه ها</a></li>
                                                    <li><a href="features.html#progress">نوار های پیشرفت</a></li>
                                                    <li><a href="features.html#tables">جداول</a></li>
                                                </ul>
                                            </li>
                                        </ul>
                                    </li>
                                    <li><a href="about-us.html">درباره ما</a></li>
                                    <li class="active"><a href="contact.html">تماس با ما</a></li>
                                </ul>

                                <!-- ========== -->
                                <!-- = Search form = -->
                                <!-- ========== -->
                                <form class="navbar-form pull-right" action="#" method="get">
                                    <button type="submit"><span class="icon-search"></span></button>
                                    <input type="text" class="span1" name="search" id="navSearchInput">
                                </form>
                            </div><!-- /.nav-collapse -->
                        </div>

                        <!-- ========== -->
                        <!-- = Cart = -->
                        <!-- ========== -->

                        <div class="span3" id="cart">
                            <div class="cart-container" id="cartContainer">
                                <div class="cart">
                                    <p class="items">سبد خرید <span class="dark-clr">(@{Html.RenderAction("CartCount", "ShoppingCart");})  @*(@{Html.RenderAction(MVC.ShoppingCart.CartCount());})*@</span></p>
                                    <p class="dark-clr hidden-tablet">محصولات</p>
                                    <a href="@Url.Action(MVC.ShoppingCart.Basket())" class="btn btn-danger">
                                        <!-- <span class="icon icons-cart"></span> -->
                                        <i class="icon-shopping-cart"></i>
                                    </a>
                                </div>
                                <div class="open-panel" style="display: none;">
                                    <div class=" item-in-cart clearfix">

                                    </div>
                                    @{
                                        Html.RenderAction("TotalPrice", "ShoppingCart");

                                    }
                                </div>
                            </div>
                        </div>

                    </div> <!-- /cart -->
                </div>
            </div>
        </div>
    </div> <!-- /main menu -->
    <!-- ========== -->
    <!-- = Breadcrumbs = -->
    <!-- ========== -->
    <div class="darker-stripe">
        <div class="container">
            <div class="row">
                <div class="span12">
                    <ul class="breadcrumb">
                        @RenderSection("Url", false)
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="push-up top-equal blocks-spacer-last">
            <div class="row">
                @RenderBody()

            </div>
        </div> <!-- /container -->
        <!-- ========== -->
        <!-- = Footer = -->
        <!-- ========== -->
        @Html.Action(MVC.Home.ActionNames.Footer, MVC.Home.Name)


    </div>
    <!-- end of master-wrapper -->
    <!-- ========== -->
    <!-- = JavaScript = -->

    <script src=@Url.Content( "~/Scripts/jquery-3.1.1.min.js" )></script>


    <script src="@Url.Content( "~/Scripts/jquery-1.9.1.js")"></script>
    @*<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>*@
    <script type="text/javascript">
        if (typeof jQuery == 'undefined') {
            document.write('<script src="js/jquery.min.js"><\/script>');
        }
    </script>

    <!-- = _ = -->
    <script src="@Url.Content( "~/Content/WebMarket/webmarket/js/underscore/underscore-min.js" )" type="text/javascript"></script>

    <!-- = Bootstrap = -->
    <script src="@Url.Content( "~/Content/WebMarket/webmarket/js/bootstrap.min.js" )" type="text/javascript"></script>

    <!-- = Slider Revolution = -->
    <script src="@Url.Content( "~/Content/WebMarket/webmarket/js/rs-plugin/pluginsources/jquery.themepunch.plugins.min.js" )" type="text/javascript"></script>
    <script src="@Url.Content( "~/Content/WebMarket/webmarket/js/rs-plugin/js/jquery.themepunch.revolution.min.js" )" type="text/javascript"></script>

    <!-- = CarouFredSel = -->
    <script src="@Url.Content( "~/Content/WebMarket/webmarket/js/jquery.carouFredSel-6.2.1-packed.js" )" type="text/javascript"></script>

    <!-- = jQuery UI = -->
    <script src="@Url.Content( "~/Content/WebMarket/webmarket/js/jquery-ui-1.10.3/js/jquery-ui-1.10.3.custom.min.js" )" type="text/javascript"></script>
    <script src="@Url.Content( "~/Content/WebMarket/webmarket/js/jquery-ui-1.10.3/touch-fix.min.js" )" type="text/javascript"></script>

    <!-- = Isotope = -->
    <script src="@Url.Content( "~/Content/WebMarket/webmarket/js/isotope/jquery.isotope.min.js" )" type="text/javascript"></script>

    <!-- = Tour = -->
    <script src="@Url.Content( "~/Content/WebMarket/webmarket/js/bootstrap-tour/build/js/bootstrap-tour.min.js")" type="text/javascript"></script>

    <!-- = PrettyPhoto = -->
    <script src="@Url.Content( "~/Content/WebMarket/webmarket/js/prettyphoto/js/jquery.prettyPhoto.js")" type="text/javascript"></script>

    <!-- = Google Maps API = -->
    <script type="text/javascript" src="@Url.Content( @"~/Content/WebMarket/webmarket/http://maps.google.com/maps/api/js?sensor=false")"></script>
    <script type="text/javascript" src="@Url.Content( "~/Content/WebMarket/webmarket/js/goMap/js/jquery.gomap-1.3.2.min.js")"></script>

    <!-- = Custom JS = -->
    <script src="@Url.Content( "~/Content/WebMarket/webmarket/js/custom.js")" type="text/javascript"></script>




    <script src=@Url.Content( "~/Scripts/Noty/jquery.noty.js" )></script>
    <script src=@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")></script>
    <script src=@Url.Content("~/Scripts/jquery.validate.min.js")></script>
    <script src=@Url.Content( "~/Scripts/jquery.validate.unobtrusive.min.js" )></script>
    <script src=@Url.Content("~/Scripts/PersianDateTimePicker/PersianDatePicker.min.js")></script>


    @RenderSection("Scripts", false)




</body>
</html>
